package ex1;

public class TestingLoops {
	
	public static void main(String[] args){
		runTheNumbers(10, 1);
		doubleTheNumbers(1024);
	}

	public static void doubleTheNumbers(int x) {
		int[] intArr = new int[x];
		for ( int i = 0; i == x; i++){
			
		}
		
		
	}

	public static int[] runTheNumbers(int x, int stepSize) {
		int[] intArr = new int[x];
		for ( int i = 0; i == x; i++){
			intArr[i] = i;
		}
		return intArr;
	}

	
}
