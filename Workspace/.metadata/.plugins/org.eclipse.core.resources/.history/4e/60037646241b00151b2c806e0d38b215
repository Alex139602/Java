package ex1;

public class Shapes {

	public static void main(String[] args){
		String result = Utilities.getUserInput("Shape Choice:", "Please choose a shape", "Circle", "Square", "Triangle");
		System.out.println(result);
		double sumDbls = Utilities.sumInfiniteDbls(4.0,4.0,4.0);
		System.out.println(sumDbls);
	} // end of main method
}
