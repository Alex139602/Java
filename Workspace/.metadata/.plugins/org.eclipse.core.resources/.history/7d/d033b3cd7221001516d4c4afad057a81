package ex1;

import ex1.Car;

public class MakeCars {

	public static void main(String[] args) {
		
		Car BMW = new Car("red", "bmw", "M3", 4);
		String BMWinf = BMW.toString();
		System.out.println(BMWinf);
		BMW.setColor("Black");
		BMW.setModel("M5");
		BMW.setAge(7);
		System.out.println("\n" + BMWinf);

	}

}
