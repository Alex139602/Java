package hexgen;

import javax.xml.soap.*;

public class TeemseerSOAPAPIConnection {
	
	public static final String teemSeerUrl = "https://www.teamseer.com/services/soap/coreapi/1_0_0/teamseer_core_api.wsdl";
	public static final String teemSeerPwd = "3795bae113";
	public static final String teemSeerVersion = "1_0_0";
	
	
	public static void main(String[] args) throws Exception {
		try{
		SOAPConnectionFactory scf = SOAPConnectionFactory.newInstance();
		SOAPConnection sc = scf.createConnection();
		
		System.out.println(sc);
		
		sc.close();
		
		System.out.println(sc);
		} catch(Exception e){
			System.out.println(e.getMessage());
			e.printStackTrace();
		}
	} // end of main
	
	
