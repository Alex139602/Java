package ex1;

import java.util.Arrays;

public class TestingLoops {
	
	public static void main(String[] args){
		System.out.println(runTheNumbers(10, 1));
		doubleTheNumbers(1024);
	}

	public static void doubleTheNumbers(int x) {
		int[] intArr = new int[x];
		for ( int i = 0; i == x; i++ ){
			
		}
		
		
	}

	public static String runTheNumbers(int x, int stepSize) {
		int[] intArr = new int[x];
		for ( int i = 0; i == x; i += stepSize ){
			intArr[i] = i;
		}
		return Arrays.toString(intArr);
	}

	
}
