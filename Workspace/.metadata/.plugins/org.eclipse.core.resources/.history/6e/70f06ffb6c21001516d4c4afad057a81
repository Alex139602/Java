package ex1;

public class Car {

	//double engineSize;
	//int chassisNumber;
	String color;
	String make;
	String model;
	int age; 
	
	// each class has a public method called constructor which is named with the class name.
	
	public Car(String c, String m, int a) {
		color = c;
		make = m;
		age = a;
	}

	public String toString() {
		return "Color: " + color + "\n" + "Make: " + make;
	}










} // end of class
