package ex1;

public class Arrays {
	public static void main(String[] args){
		
		String[] str = arrayTest(5);
		System.out.println(str);
		
	}
	
	public static String[] arrayTest(int arrayLength){
		String[] stArr = new String[arrayLength];
		for (int i = 0; i < stArr.length; i++) {
			stArr[i] = "Slot " + i;
		}
		
		
		return stArr;
	}
}
