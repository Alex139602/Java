package money;

public class getMoney {

	public static void main(String[] args) {
		
		Account acc = new Account("Alex", 1000);
		acc.withdraw(500);
		System.out.println( acc.getHolder() + ", " + acc.getBalance() + ", " + acc.getAccountNumber() );
		Account urm = new Account("Urmila");
		urm.accountToString();
		
	} // end of main

	
	
	
	
	
	
	
	
	
	
	
	
	
	
} // end of class
